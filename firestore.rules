rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /users/{userId} {
      allow read, write, create: if request.auth.token.email != null
    }
    match /todo/{todo} {
      allow write: if request.resource.data.done == false
      allow read: if request.auth == null
      // allow create: 
    }
  }
}